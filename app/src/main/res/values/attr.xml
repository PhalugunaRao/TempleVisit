<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="loader_view">
        <attr name="width_weight" format="float" />
        <attr name="height_weight" format="float" />
        <attr name="use_gradient" format="boolean" />
    </declare-styleable>
    <declare-styleable name="RalewayTextViewMedium">
        <attr name="ralewaytypeface" />
    </declare-styleable>
    <declare-styleable name="AvenirTextView">
        <attr name="avenirtypeface" />
    </declare-styleable>
    <declare-styleable name="ProximaNovaTextView">
        <attr name="typeface" />
    </declare-styleable>




    <declare-styleable name="ExpandableLayout">
        <attr name="el_headerLayout" format="reference" />
        <attr name="el_contentLayout" format="reference" />
        <attr name="el_duration" format="integer" />
    </declare-styleable>



     <attr name="avenirtypeface" format="enum">
        <enum name="avenir_black" value="0" />
        <enum name="avenir_black_oblique" value="1" />
        <enum name="avenir_book" value="2" />
        <enum name="avenir_book_oblique" value="3" />
        <enum name="avenir_heavy" value="4" />
        <enum name="avenir_heavy_oblique" value="5" />
        <enum name="avenir_light" value="6" />
        <enum name="avenir_light_oblique" value="7" />
        <enum name="avenir_medium" value="8" />
        <enum name="avenir_medium_oblique" value="9" />
        <enum name="avenir_oblique" value="10" />
        <enum name="avenir_roman" value="11" />
    </attr>

    <attr name="ralewaytypeface" format="enum">
        <enum name="raleway_bold" value="0" />
        <enum name="raleway_extra_bold" value="1" />
        <enum name="raleway_extra_light" value="2" />
        <enum name="raleway_heavy" value="3" />
        <enum name="raleway_light" value="4" />
        <enum name="raleway_medium" value="5" />
        <enum name="raleway_regular" value="6" />
        <enum name="raleway_semi_bold" value="7" />
        <enum name="raleway_thin" value="8" />
    </attr>
    <attr name="checkboxtypeface" />
    <attr name="typeface" format="enum">
        <enum name="proxima_regular" value="0" />
        <enum name="proxima_bold" value="1" />
        <enum name="proxima_extra_bold" value="2" />

    </attr>
    <attr name="centered" format="boolean" />
    <attr name="selectedColor" format="color" />
    <attr name="strokeWidth" format="dimension" />
    <attr name="unselectedColor" format="color" />
    <attr name="vpiCirclePageIndicatorStyle" format="reference" />
   <declare-styleable name="FloatingActionButton">
        <attr name="fab_colorPressed" format="color"/>
        <attr name="fab_colorDisabled" format="color"/>
        <attr name="fab_colorNormal" format="color"/>
        <attr name="fab_icon" format="reference"/>
        <attr name="fab_size" format="enum">
            <enum name="normal" value="0"/>
            <enum name="mini" value="1"/>
        </attr>
        <attr name="fab_title" format="string"/>
        <attr name="fab_stroke_visible" format="boolean"/>
    </declare-styleable>
    <declare-styleable name="AddFloatingActionButton">
        <attr name="fab_plusIconColor" format="color"/>
    </declare-styleable>
    <declare-styleable name="FloatingActionsMenu">
        <attr name="fab_addButtonColorPressed" format="color"/>
        <attr name="fab_addButtonColorNormal" format="color"/>
        <attr name="fab_addButtonSize" format="enum">
            <enum name="normal" value="0"/>
            <enum name="mini" value="1"/>
        </attr>
        <attr name="fab_addButtonPlusIconColor" format="color"/>
        <attr name="fab_addButtonStrokeVisible" format="boolean"/>
        <attr name="fab_labelStyle" format="reference"/>
        <attr name="fab_labelsPosition" format="enum">
            <enum name="left" value="0"/>
            <enum name="right" value="1"/>
        </attr>
        <attr name="fab_expandDirection" format="enum">
            <enum name="up" value="0"/>
            <enum name="down" value="1"/>
            <enum name="left" value="2"/>
            <enum name="right" value="3"/>
        </attr>
    </declare-styleable>

    <declare-styleable name="ComboSeekBar">
        <attr name="multiline" format="boolean" />
        <attr name="myColor" format="color" />
        <attr name="textSize" format="dimension" />
    </declare-styleable>

    <declare-styleable name="TagFlowLayout">
        <attr name="auto_select_effect" format="boolean"></attr>
        <attr name="max_select" format="integer"></attr>
        <attr name="gravity">
            <enum name="left" value="-1" />
            <enum name="center" value="0" />
            <enum name="right" value="1" />
        </attr>
    </declare-styleable>

    <declare-styleable name="Theme">
        <attr name="circularImageViewStyle" format="reference"/>
    </declare-styleable>
    <declare-styleable name="CircularImageView">
        <attr name="border" format="boolean"/>
        <attr name="border_width" format="dimension"/>
        <attr name="border_color" format="color"/>
        <attr name="shadow" format="boolean"/>
    </declare-styleable>

    <declare-styleable name="ButtonBarContainerTheme">
        <attr name="metaButtonBarStyle" format="reference" />
        <attr name="metaButtonBarButtonStyle" format="reference" />
    </declare-styleable>

    <declare-styleable name="FixedAspectRatioImageView">
        <attr name="widthToHeightRatio" format="float"/>
    </declare-styleable>

    <declare-styleable name="FixedAspectRatioRoundedImageView">
        <attr name="roundedImageView_widthToHeightRatio" format="float"/>
    </declare-styleable>

    <declare-styleable name="CircleCheckbox">
        <attr name="showOuterCircle" format="boolean" />
        <attr name="tickColor" format="string"/>
        <attr name="outerCircleColor" format="string"/>
        <attr name="innerCircleColor" format="string"/>
        <attr name="textColor" format="string"/>
        <attr name="circleBorderColor" format="string"/>
        <attr name="outerCircleRadius" format="dimension"/>
        <attr name="innerCircleRadius" format="dimension"/>
        <attr name="borderThickness" format="dimension"/>
        <attr name="tickThickness" format="dimension"/>
        <attr name="textLeftPadding" format="dimension"/>
        <attr name="text" format="string"/>
    </declare-styleable>

    <declare-styleable name="HealthScoreView">
        <attr name="circleColor" format="color" />
        <attr name="circleLabel" format="string"></attr>
        <attr name="textLabelColor" format="color"></attr>
    </declare-styleable>

</resources>